#include <stdio.h>
/**
 *main - Entry Point
 *lower and uppercase
 *Return: always 0
 */
int main(void)
{
	int n = 97;
        int n = 65;

        while (n <= 122)
        {
                putchar(n);
        }
        while (n <= 90)
        {
                putchar(n);
        }
	putchar('\n');
	return (0);
}
